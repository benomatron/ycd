<% include ../partials/header %>
<div class="container">
    <div class="row">
        <div class="camp-form">
            <h1>new camp</h1>
            <form action='/camps' method='post'>
                <div class="form-group">
                    <input class="form-control" type='text' name='camp-name' placeholder='camp name'>
                </div>
                <div class="form-group">
                    <input class="form-control" type='text' name='img-url' placeholder='image url'>
                </div>
                <div class="form-group">
                    <input class="form-control" type='text' name='desc' placeholder='description'>
                </div>
                <div class="form-group">
                    <input class="btn btn-primary btn-large btn-block" type='submit'>
                </div>
            </form>
        </div>
    </div>
</div>
<% include ../partials/footer %>
